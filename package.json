  "jest": {
    "collectCoverageFrom": [
      "**/*.{js,vue}",
      "!**/node_modules/**",
      "!**/coverage/**",
      "!**/webpack.config.js",
      "!**/bundles/**"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleNameMapper": {
      "^vue$": "vue/dist/vue.common.js",
      ".+\\.(css|styl|less|sass|scss)$": "identity-obj-proxy",
      ".+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/foodalert/static/foodalert/js/__mocks__/fileMock.js"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.js": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "vue-jest"
    }
  }
}
